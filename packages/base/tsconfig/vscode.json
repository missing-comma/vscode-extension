{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "VsCode",
	"extends": "./base.json",
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"lib": ["ES2020"],
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true
	},
	"exclude": ["node_modules"]
}
